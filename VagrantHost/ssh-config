Host network
    HostName 127.0.0.1
    User vagrant
    Port 22
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    PasswordAuthentication no
    IdentityFile ~/.ssh/machines/network/virtualbox/private_key
    IdentitiesOnly yes
    LogLevel FATAL
  
