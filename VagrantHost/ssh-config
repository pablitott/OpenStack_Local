Host controller
    HostName 192.168.100.11
    User vagrant
    Port 22
    IdentityFile ~/.ssh/machines/controller/virtualbox/private_key
    IdentitiesOnly yes
    LogLevel FATAL
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

Host compute1
    HostName 192.168.100.21
    User vagrant
    Port 22
    IdentityFile ~/.ssh/machines/compute1/virtualbox/private_key
    IdentitiesOnly yes
    LogLevel FATAL
    StrictHostKeyChecking no
    UserKnownHostsFile=/dev/null

